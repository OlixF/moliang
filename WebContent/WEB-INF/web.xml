<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>mo</display-name>
  <servlet>
    <servlet-name>UserAction</servlet-name>
    <servlet-class>com.moliang.Action.UserAction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserAction</servlet-name>
    <url-pattern>/user</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>GoodsAction</servlet-name>
    <servlet-class>com.moliang.Action.GoodsAction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GoodsAction</servlet-name>
    <url-pattern>/goods</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>CarAction</servlet-name>
    <servlet-class>com.moliang.Action.CarAction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CarAction</servlet-name>
    <url-pattern>/car</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>